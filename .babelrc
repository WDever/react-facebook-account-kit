{
  "env": {
    "development": {
      "presets": [
        [
          "es2015",
          {
            "modules": false
          }
        ],
        "react"
      ]
    },
    "test": {
      "presets": [
        "es2015",
        "react"
      ]
    },
    "production": {
      "presets": [
        [
          "es2015",
          {
            "modules": false
          }
        ],
        "react"
      ],
      "plugins": [
        "transform-react-remove-prop-types",
        "transform-react-constant-elements",
        "transform-react-inline-elements"
      ]
    }
  }
}